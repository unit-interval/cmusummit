<div id="content" class="clearfix">
<div class="section">
<div class="article" id="admin-tool">
<h1>Presentations Model</h1>
<div>
<table>
  <tr>
	<th>ID</th>
    <th>Title</th>
    <th>Type</th>
    <th></th>
  </tr>

<% @presentations.each do |presentation| %>
  <tr>
	<td><%= presentation.id %></td>
    <td><%= presentation.title %></td>
    <td><%= presentation.datatype %></td>
    <td><%= link_to 'Show', presentation %> <%= link_to 'Edit', edit_presentation_path(presentation) %> <%= link_to 'Destroy', presentation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<%= link_to 'New Presentation', new_presentation_path %>
</div></div></div></div>