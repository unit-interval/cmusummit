#agenda .info-dot {
	position: absolute;
	height: 14px;
	width: 14px;
	border-radius: 7px;
	-moz-border-radius: 7px;
	top: 9px;
	background: #666;
	z-index: 1;
}
#agenda .info-balloon {
	background: transparent url(<%= image_path 'agenda-info-balloon.png' %>) no-repeat top left;
	position: absolute;
	width: 242px;
	height: 79px;
	padding: 10px;
	z-index: 99;
	top: -87px;
	left: -33px;
	line-height: 20px;
	display: none;
}
#agenda .info-balloon h4 {
	font-size: 15px;
	font-weight: normal;
	color: #5b4f31;
	margin: 0 0 3px 0;
	padding: 0;
}
#agenda .info-balloon p {
	font-size: 15px;
	font-style: italic;
	color: #978149;
	margin: 0 0 3px 0;
	padding: 0;
}
#agenda .session-interested {
	left: 605px;
	bottom: 0px;
	position: absolute;
}
#agenda .session-interested .button-gray-inactive {
	margin-top: 10px;
}
.people-interested {
	margin-bottom: 10px;
}
.people-interested a.button-gray {
	margin-left: -5px;
}
/* sessions */
#agenda .session-detail {
	margin: 0 -40px;
}
#agenda .session-detail:first-child {
	margin-top: -40px;
}
#agenda .session-detail:last-child {
	margin-bottom: -40px;
}
#agenda .session-detail .session-info{
	padding: 40px;
	border-bottom: 1px dotted #D2D2D2;
}
#agenda .session-detail:last-child {
	border: none;
}
#agenda .session-detail h2 {
	font-size: 25px;
	color: #CF242A;
	margin-bottom: 4px;
}
#agenda .session-detail h3 {
	font-size: 24px;
	font-weight: normal;
	margin: 0 0 10px 0;
}
#agenda .session-detail .speaker-list {
    color: #363636;
    font-size: 18px;
    line-height: 1.2;
    margin: 0 0 24px;
}
#agenda .session-detail .description-container {
	min-height: 120px;
    overflow: hidden;
    width: 100%;
	position: relative;
}
#agenda .session-detail .description {
    border-right: 1px dotted #d2d2d2;
    color: #939393;
    float: left;
    font-size: 18px;
    line-height: 1.4;
    margin: 0;
    padding-right: 26px;
    width: 540px;
}
#agenda .session-detail .session-stats {
    color: #363636;
    font-size: 13px;
    line-height: 1.8;
    float: right;
    margin: 0 0 0 26px;
    padding: 0;
    width: 255px;
}
#agenda .session-detail .session-stats span {
    color: #939393;
    margin-left: 12px;
}
.speaker-info {
    background: #f3f3f3;
    padding: 40px;
}
#agenda .speaker-info {
	border-bottom: 1px dotted #D2D2D2;
}
.speaker-info .bio {
	margin-left: 142px;
}
.speaker-info .bio .read-more {
	font-size: 13px;
}
.speaker-info .bio p {
	margin-top: 13px;
	display: none;
}
.speaker-info:nth-child(2n+1) {
    background: transparent;
}
.speaker-info .speaker {
    overflow: hidden;
    width: 100%;
}
.speaker-info .has-image .image {
    border: 1px solid #c5c5c5;
    background-color: #fff;
    float: left;
    height: 120px;
    margin-right: 20px;
    width: 120px;
}
.speaker-info .has-image .image img {
	height: 100px;
	width: 100px;
	margin: 10px;
}
.session-detail .speaker-info .bio h4 {
    color: #363636;
    font-size: 13px;
    line-height: 1.5;
    margin: 0;
}
/* people */
#people h1 > a {
	width: 313px;
	text-align: center;
	display: inline-block;
}
#people h1 > a:last-child {
	width: 314px;
}
#people > h1 > a:hover, #people > h1 > a.active {
	color: #363636
}
#people #speaker p {
	padding: 40px;
}
#people .people-page {
	display: none;
}
#people .people-page.active {
	display: block;
}
.people-page h3 {
	font-size: 24px;
	font-weight: normal;
	margin: 0 0 10px 0;
}
.people-page h4, .people-page p, #team .bio h4, #team .bio p {
	color: #363636;
	font-size: 13px;
	line-height: 1.5;
	margin: 0;
}
#participant {
	padding: 40px 40px 20px;
}
#participant .speaker-info, #my-contact .speaker-info, #team .speaker-info {
	padding: 0px;
	background: transparent;
	float: left;
	width: 286px;
	margin-bottom: 20px;
}
#participant .image, #my-contact .image, #team .image {
	width: 60px;
	height: 60px;
	margin-right: 10px;
	-webkit-transition: border-color 0.2s ease;
	-moz-transition: border-color 0.2s ease;
	-o-transition: border-color 0.2s ease;
	-ms-transition: border-color 0.2s ease;
	transition: border-color 0.2s ease;
}
#participant .image:hover, #my-contact .image:hover, #team .image:hover {
	border-color: #CF242A;
}
#participant h3, #my-contact h3, #team h3 {
	font-size: 20px;
	margin: 0 0 5px 0;
}
#participant .bio, #my-contact .bio, #team .bio, #team .bio {
	margin-bottom: 10px;
	margin-left: 82px;
}
#participant .image img, #my-contact .image img, #team .image img{
	height: 50px;
	width: 50px;
	margin: 5px;
}
/* contest */
#contest h1 > a {
	width: 313px;
	text-align: center;
	display: inline-block;
}
#contest h1 > a:last-child {
	width: 314px;
}
#contest > h1 > a:hover, #contest > h1 > a.active {
	color: #363636
}
#contest h2 {
	color: #363636;
	font-size: 25px;
	margin: 0 0 10px 0;
}
#contest p, #contest ul, #contest ol, #contest table, #contest dd {
	font-size: 15px;
	color: #363636;
	margin-bottom: 20px;
}
#contest dt {
	font-size: 15px;
	margin-bottom: 10px;
}

#contest .content {
	display: none;
}
#contest .content.active {
	display: block;
}
/* faq */
#faq div.faq-section {
    width: 45%;
    float: left;
    margin-right: 40px;
}
#faq div.right {
    float: right;
    margin-right: 0;
}
#faq h3 {
	font-size: 24px;
}
#faq dt {
	font-size: 13px;
    font-weight: bold;
    margin-top: 25px;
}
#faq dd {
    font-size: 13px;
    margin: 5px 0 0 0;
    padding: 0;
    line-height: 1.4em;
}
/* info */
#info > h1 > a {
	width: 25%;
	display: inline-block;
	text-align: center;
}
#info > h1 > a:hover, #info > h1 > a.active {
	color: #363636
}
#info #about > p {
	font-size: 22px;
	color: #939393;
	font-weight: normal;
	line-height: 32px;
}
#info #about-left {
	float: left;
	width: 400px;
}
#info #about-right {
	float: right;
	width: 400px;
}
#info #about .about-item {
	margin: 40px 20px;
}
#info h2 {
	color: #363636;
	font-size: 25px;
	margin: 0 0 10px 0;
}
#info #travel-campus, #info #pricing {
	width: 420px;
	float: left;
}
#info #travel-hotel, #info #questions {
	width: 420px;
	float: right;
}
#info #travel p, #info #travel h3 {
	margin-bottom: 10px;
}
#info #travel h3 {
	font-weight: 600;
}
#info #travel-details ul {
	margin-bottom: 10px;
}
#info #travel-details li {
	line-height: 1.4em;
}
#info #register > p {
	margin-bottom: 20px;
}
#info #register table {
	font-size: 13px;
	width: 100%;
}
#info #register td {
	padding: 5px 0;
}
#info #register #register-button {
	display: block;
	width: 200px;
	line-height: 50px;
	font-size: 25px;
	font-weight: normal;
	margin: 0 auto 20px;
}
#info .content {
	display: none;
}
#info .content.active {
	display: block;
}
/* register */
#register h1 {
	padding: 20px;
}
#register #forget-notification, #register #error-signup, #register #error-signin {
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	line-height: 23px;
	padding: 8px;
	font-size: 13px;
	background: #DDD;
	text-align: center;
	margin-bottom: 20px;
}
#register h1 span {
	width: 50%;
	display: inline-block;
	text-align: center;
}
#register #login, #register #signup {
	width: 390px;
	float: left;
	padding: 40px;
}
#register #signup {
	float: right;
	width: 389px;
	border-left: 1px dotted #D2D2D2
}
#register form {
	font-size: 13px;
}
#register label {
	width: 80px;
	text-align: right;
	padding: 20px 10px 20px 0;
	display: inline-block;
}
#register .field_with_errors label {
  color: #ff4500;
}
#register form div {
	position: relative;
}
#register form span {
	position: absolute;
	right: 30px;
	top: 16px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	line-height: 23px;
	padding: 0 8px;
	font-size: 12px;
	background: #DDD;
}
#register #forget {
	display: none;
	margin-top: 40px;
	padding-top: 20px;
	border-top: 1px dashed #D2D2D2;
}
#register input[type="text"], #register input[type="password"], #register input[type="email"] {
	width: 250px;
	margin: 2px;
}
#register .field_with_errors input {
  border: 2px solid #ff4500;
}
/* todo */
#my-profile, #connect-social-network {
	font-size: 13px;
}
#my-profile label, #connect-social-network label{
	width: 90px;
	text-align: right;
	padding: 10px 20px 10px 0;
	display: inline-block;
}
#my-profile input[type="text"], #my-profile input[type="password"]{
	width: 300px;
}
#connect-social-network .button-gray{
	width: 100px;
}
#update-profile .social-network-link {
	display: none;
	position: relative;
}
#update-profile .social-network-link input[type="text"]{
	padding-left: 130px;
	margin: 6px 0px;
	float: none;
	width: 100px;
}
#update-profile .social-network-link label {
	position: relative;
	top: -38px;
	left: 8px;
}
#update-profile .social-network-link input[type="submit"]{
	margin-left: 1px;
	width: 117px;
	float: right;
}
#update-profile form {
	font-size: 13px;
}
#update-profile label {
	width: 90px;
	padding: 10px 0;
	display: inline-block;
}
#update-profile input[type="text"], #update-profile input[type="password"]{
	width: 125px;
	margin: 2px;
    float: right;
}
#update-profile input[type="radio"] {
    margin-left: 10px;
}
#update-profile #connect-social-network div.button-gray {
	margin: 5px auto 0px;
	line-height: 18px;
}
#update-profile .content {
	max-height: 9999px;
}
#update-profile .image {
    border: 1px solid #c5c5c5;
    background-color: #fff;
    float: left;
    margin-right: 15px;
    height: 60px;
    width: 60px;
}
#update-profile .image img {
	height: 50px;
	width: 50px;
	margin: 5px 5px 5px 5px;
}
#update-info span {
	font-weight: bold;
}
#update-info.active span, #update-info.active #change-profile-button, #update-info.active span, #update-info #save-changes {
	display: none;
}
#update-info.active input[type="text"], #update-info.active input[type="radio"], #update-info.active .gender-span {
	display: inline;
	font-weight: normal;
}
#update-info.active #save-changes {
	display: inline-block;
}
#change-password-button {
	float: right;
}
#update-info input[type="text"], #update-info input[type="radio"], #update-info .gender-span {
	display: none;
}
.change-profile-button-group {
	margin-top: 10px;
}
#update-password-div {
	display: none;
}
#my-calendar{
	font-size: 13px;
	line-height: 20px;
}
#my-calendar h2{
	color: #CF242A;
}
#my-calendar .session-info {
	padding: 10px 20px;
	margin: 20px 0;
	border-left: 5px solid #EEE;
}
#my-contact .speaker-info {
	padding: 15px 20px 8px;
	margin: 20px 0;
	border-left: 5px solid #EEE;
	float: none;
	width: auto;
	font-size: 13px;
	line-height: 20px;
}
#my-contact .speaker-info .image {
	margin-right: 20px;
}
#my-contact .speaker-info h3, #my-contact .speaker-info h4{
	font-size: 13px;
	margin: 0;
}
.todo-remove {
	float: right;
	border-bottom: 1px dotted #CF242A;
	margin-top: -21px;
	display: none;
}
#my-contact .speaker-info .todo-remove {
	margin-top: -28px;
}
#my-contact .speaker-info:hover, #my-calendar .session-info:hover {
	background-color: #EEE;
}
.session-info:hover .todo-remove, .speaker-info:hover .todo-remove {
	display: block;
}
/* team */
#advisor h1, #team h1 {
	padding: 20px;
	text-align: center;
}
#advisor div.content {
	padding: 0;
}
#team-desc {
	margin-bottom: 20px;
}
#team div.content {
	padding-bottom: 20px;
}
/* footer */
div.footer {
	padding: 30px;
	font-size: 11px;
	text-align: center;
	color: #767676;
	line-height: 16px;
	background: #F9F9F7 url(<%= image_path 'sand-bg.png' %>);
	border-top: 1px solid #DBDCDE;
}
div.footer a:hover {
	color: black;
}
/* override rails field_error_proc */
form div.field_with_errors {
  display: inline;
}
