	background-image: -webkit-linear-gradient(top,#CF242A,#CF242A 25%,#900);
	background-image: -o-linear-gradient(top,#CF242A,#900);
	background-image: -ms-linear-gradient(#CF242A,#900);
	background-image: linear-gradient(#CF242A,#900);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#CF242A, endColorstr=#900, GradientType=0);
	border: 1px solid #600;
	color: white;
	cursor: pointer;
	letter-spacing: 0;
	height: 20px;
	line-height: 20px;
	padding: 5px 15px 4px;
	text-align: center;
